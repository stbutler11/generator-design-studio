{
    "dist": {
        "options": {
            "patterns": [{
                "match": "bundle",
                "replacement": "<%%= bundle %>"
            }, {
                "match": "version",
                "replacement": "<%%= version %>"
            }, {
                "match": "titleLower",
                "replacement": "<%%= sdkNameLower %>"
            }, {
                "match": "titleOneWord",
                "replacement": "<%%= sdkNameOneWord %>"
            }, {
                "match": "ds_version",
                "replacement": "<%%= dsVersion %>"
            }, {
                "match": "ds_base_version",
                "replacement": "<%%= dsBaseVersion %>"
            }, {
                "match": "timestamp",
                "replacement": "<%%= timestamp %>"
            }]
        },
        "files": [{
            "expand": true,
            "flatten": true,
            "src": "src/feature_files/*",
            "dest": ".tmp/feature_files/"
        }, {
            "expand": true,
            "flatten": true,
            "src": "src/component/META-INF/MANIFEST.MF",
            "dest": ".tmp/component/META-INF/"
        }, {
            "expand": true,
            "flatten": true,
            "src": "src/component/contribution.xml",
            "dest": ".tmp/component"
        }]
    }
}
